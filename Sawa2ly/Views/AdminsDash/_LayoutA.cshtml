
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link href="~/Sources/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <script src="~/Sources/css/bootstrap.min.js"></script>
    <script src="~/Sources/jquery.min.js"></script>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" href="~/Sources/fontawesome-free-5.8.1-web/css/all.css" crossorigin="anonymous">
</head>
<body>
    @Html.Partial("_header")

    <div class="container">
        <div class="row">
            <div class="col-md-3">
                <div class="page-wrapper chiller-theme toggled">
                    @Html.Partial("~/Views/AdminsDash/_navA.cshtml")
                </div>
            </div>
            <div class="col-md-9">
                @RenderBody()
            </div>
        </div>
    </div>
    <hr />
</body>
</html>
